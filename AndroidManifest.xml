<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scubclient"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.scubclient.MainActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".JwcActivity"></activity>
        <activity android:name=".InfoContextActivity"></activity>
        <activity android:name=".QgActivity"></activity>
        <activity android:name=".JzActivity"></activity>
        <activity android:name=".LfActivity"></activity>
        <activity android:name=".LfPublishActivity"></activity>
        <activity android:name=".PublishActivity"></activity>
        <activity android:name=".JzPublishActivity"></activity>
        <activity android:name=".ShowJzActivity"></activity>
        <activity android:name=".ShowLfActivity"></activity>
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    
</manifest>
